
public class PrintNumberInWord {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// Print Number In Word
		
		int number = 33;
		
		number1 = 1;
		number2 = 2;
		number3 = 3;
		number4 = 4;
		
		if(number = 1){
			System.out.println("ONE");
						
		}else if(){
			System.out.println("TWO");
						
		}else{
			System.out.println("THREE");
		
		System.out.println();

	}

}
